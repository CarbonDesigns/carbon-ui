<body>
<div id="reactContainer"></div>
<script type="text/javascript">
    //auto-generated
    window.endpoints = {
        services: '',
        storage: '',
        cdn: '',
        file: ''
    };
    window.telemetryKey = '';
    window.appBuild = '1.0.0';

    window.c = {
        coreCallback: null,
        coreLoaded: false,
        apiLoaded: false,
        api: null,
        core: null,
        coreScript: "<%=htmlWebpackPlugin.options.coreScript%>",
        resourceFile: "reslist.json" //replaced by hashing
    };

    window.__carbonapi = function(api){
        var carbon = window.c;
        carbon.apiLoaded = true;
        carbon.api = api;
    }
    window.__carboncore = function(core){
        var carbon = window.c;
        carbon.coreLoaded = true;
        carbon.core = core;
        if (carbon.coreCallback){
            carbon.coreCallback();
        }
    }
</script>

<!-- needed for mobile safari -->
